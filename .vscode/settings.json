{
    "unocss.root": ["frontend"],
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "prettier.semi": false,
    "prettier.tabWidth": 4,
    "prettier.printWidth": 100,
    "prettier.bracketSameLine": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[go]": {
        "editor.defaultFormatter": "golang.go"
    }
}
